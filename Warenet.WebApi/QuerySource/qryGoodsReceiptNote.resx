  <data name="selectNewGrnNo" xml:space="preserve">
    <value>DECLARE @GrnMth varchar(20) = CONVERT(CHAR(4), @ReceiptDate, 12)
DECLARE @LastSeqNo int;
DECLARE @NewGrnNo nvarchar(20);

SELECT @LastSeqNo = ISNULL(MAX(CONVERT(INT,RIGHT(GoodsReceiptNoteNo,5))),0)
FROM whgr1 
WHERE SUBSTRING(GoodsReceiptNoteNo,4,4) = CONVERT(CHAR(4), @ReceiptDate, 12)

SET @LastSeqNo = @LastSeqNo + 1;
SET @NewGrnNo = 'GRN' + @GrnMth + RIGHT('00000' + CONVERT(VARCHAR,@LastSeqNo),5);
SELECT @NewGrnNo</value>
  </data>
  <data name="selectPutAwayItem" xml:space="preserve">
    <value>SELECT	gr2.TrxNo,
		gr2.LineItemNo,
		0 AS IsSelected,
		gr1.GoodsReceiptNoteNo,
		gr2.ItemCode,
		gr2.[Description],
		gr2.BinNo,
		gr2.DimensionFlag,
		gr2.UomCode,
		gr2.Qty,
		gr2.[Length],
		gr2.Width,
		gr2.Height,
		gr2.Width,
		gr2.Volume,
		gr2.SpaceArea,
		gr2.ManufactureDate,
		gr2.ExpiryDate
FROM whgr1 gr1 INNER JOIN whgr2 gr2 ON gr1.TrxNo = gr2.TrxNo
WHERE	gr1.StatusCode NOT IN ('CNF','DEL') AND 
		ISNULL(gr2.BinNo,'') = '' AND
		gr1.WarehouseCode = @WarehouseCode AND
		gr1.SupplierCode = @SupplierCode</value>
  </data>
